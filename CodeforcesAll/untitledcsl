/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `DAILY_ATTENDANCE` (
  `PARTY_ID` varchar(20) NOT NULL,
  `DATE_OF_ATTENDANCE` datetime NOT NULL,
  `EMPLOYEE_ID` varchar(20) DEFAULT NULL,
  `FIRST_IN_TIME` varchar(20) DEFAULT NULL,
  `LAST_OUT_TIME` varchar(20) DEFAULT NULL,
  `EMPLOYEE_COMMENTS` varchar(255) DEFAULT NULL,
  `SUPERVISOR_COMMENTS` varchar(255) DEFAULT NULL,
  `ATTENDANCE_STATUS` varchar(20) DEFAULT NULL,
  `ATTENDANCE_RESULT` varchar(20) DEFAULT NULL,
  `PROCESS_STATUS` varchar(60) DEFAULT NULL,
  `DATE_ADDED` datetime DEFAULT NULL,
  `SUPERVISOR_PARTY_ID` varchar(20) DEFAULT NULL,
  `LAST_UPDATED_STAMP` datetime DEFAULT NULL,
  `LAST_UPDATED_TX_STAMP` datetime DEFAULT NULL,
  `CREATED_STAMP` datetime DEFAULT NULL,
  `CREATED_TX_STAMP` datetime DEFAULT NULL,
  `LOCATION_IN` varchar(255) DEFAULT NULL,
  `LOCATION_OUT` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`PARTY_ID`, `DATE_OF_ATTENDANCE`),
  KEY `Daily_Att_Party` (`PARTY_ID`),
  KEY `DL_ATTNDNC_TXSTMP` (`LAST_UPDATED_TX_STAMP`),
  KEY `DL_ATTNDNC_TXCRTS` (`CREATED_TX_STAMP`),
  CONSTRAINT `Daily_Att_Party` FOREIGN KEY (`PARTY_ID`) REFERENCES `PARTY` (`PARTY_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
